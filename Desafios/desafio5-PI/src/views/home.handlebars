<div class="container">
    <div class="row">
        {{#each productos}}
        <div class="col-md-3">
            <div class="card border-0 my-3">
                <img src="{{this.thumbnails}}" class="img fluid" alt="{{this.title}}">
                <div class="card-body text-left">
                    <p class="card-text">{{this.title}} <br /> <span class="text-danger">${{this.price}}</span></p>
                </div>
            </div>
        </div>
        {{/each}}
     </div>
</div>